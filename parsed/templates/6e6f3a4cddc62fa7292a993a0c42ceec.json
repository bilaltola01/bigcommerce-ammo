{"components/common/navigation-list":"{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" >\n   <span class=\"text\">{{name}}</span><i class=\"icon1 navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-down-top\"></use></svg></i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a\n                        class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                        href=\"{{url}}\"\n                        data-collapsible=\"navPages-{{id}}\"\n                        data-collapsible-disabled-breakpoint=\"medium\"\n                        data-collapsible-disabled-state=\"open\"\n                        data-collapsible-enabled-state=\"closed\">\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-down-top\" /></svg></i>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n{{/if}}\n","components/common/navigation-menu":"<nav class=\"navPages\">\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\n        {{#each categories}}\n            <li class=\"navPages-item menu-{{id}} hidden-menu {{#if children}}hasDropdown{{/if}}\">\n                {{> components/common/navigation-list}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                 <li class=\"navPages-item navPages-item-page\">\n                    {{#if children}}\n                     <a class=\"navPages-action has-subMenu\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                     <div class=\"navPage-subMenu\">\n                            <ul class=\"navPage-subMenu-list\">\n                                {{#each children}}\n                                <li class=\"navPage-childList-item\">\n                                    <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                                {{/each}}\n                            </ul>\n                        </div>\n                    {{else}}\n                    <a class=\"navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                    {{/if}}\n                 </li>\n             {{/each}}\n        {{/unless}}\n    </ul>\n    <ul class=\"navPages-list navPages-list-mobile\">\n        <li class=\"navPages-item\">\n            <a class=\"navPages-action wishlists\" href=\"{{urls.account.wishlists.all}}\"><span class=\"text\"><svg><use xlink:href=\"#icon-heart3\" /></svg>Wish Lists</span></a>\n        </li>\n        <li class=\"navPages-item giftcertificates\">\n            <a class=\"navPages-action\" href=\"{{urls.gift_certificate.purchase}}\"><span class=\"text\"><svg><use xlink:href=\"#icon-gift\"/></svg>{{lang 'common.gift_cert'}}</span></a>\n        </li>\n        <li class=\"navPages-item phone_number\">\n            <a class=\"navPages-action\" href=\"tel:{{settings.phone_number}}\"><span class=\"text\"><svg class=\"icon\"><use xlink:href=\"#icon-telephone\"/></svg>Contact us: {{settings.phone_number}}</span></a>\n        </li>\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item currency-item\">\n                <a id=\"currency_dropdown\" href=\"#\">\n                    {{{currency_selector.active_currency_flag}}}<span class=\"text\">{{currency_selector.active_currency_code}}</span>\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-down-top\" /></svg></i>\n                </a>\n                <div class=\"navPage-subMenu\">\n                    <ul class=\"navPage-subMenu-list\">\n                        {{#each currency_selector.currencies}}\n                        <li class=\"navPage-subMenu-item\">\n                            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                {{#if is_active}}\n                                {{{flag}}} <span class=\"text\"><strong>{{name}}</strong></span>\n                                {{else}}\n                                {{{flag}}} <span class=\"text\">{{name}}</span>\n                                {{/if}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navPages-item\">\n            {{> components/common/social-links}}\n        </li>\n    </ul>\n    <div class=\"deals\">\n        <a class=\"navPages-action\" href=\"#\">Sale & Clearance</a>\n        <p>New</p>\n    </div>\n</nav>\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n"}
