{"components/common/quick-search":"{{{ snippet \"forms_search\"}}}\n<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <div id=\"search_category\">\n                <span class=\"select_category\">Looking for</span>\n            </div>\n            <div class=\"box-search\">\n            \t<svg><use xlink:href=\"#icon-search1\"></use></svg>\n            \t<input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_quickSearch'}}\" autocomplete=\"off\">\n            </div>\n            <button type=\"submit\">Search</button>\n        </div>\n    </fieldset>\n</form>\n<p class=\"text-bottom-search\">Most popular searches: Federal, 9mm Luger Winchester, 6.5 prc ammo, Full Metal Jacket ...</p>\n<div class=\"quickSearchResults\" data-bind=\"html: results\"></div>\n"}
