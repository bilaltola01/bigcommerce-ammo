{"components/themevale/home-recent-blog":"<div id=\"home-recent-blog\" class=\"themevale-section\">\r\n    <div class=\"themevale_page-heading-wrapper\">\r\n        <div class=\"themevale_container\">\r\n            <h2 class=\"themevale_page-heading\">Recent Posts</h2>\r\n            <div class=\"view_all\">\r\n                <a href=\"#\">{{lang 'common.view_all'}}<svg><use xlink:href=\"#icon-down-top\"/></svg></a>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div class=\"themevale_container\">\r\n        <div class=\"themevale_banner-list productCarousel\" data-slick='{\r\n                \"nav\": false,\r\n                \"dots\": true,\r\n                \"arrows\": false,\r\n                \"infinite\": false,\r\n                \"slidesToShow\": 2,\r\n                \"slidesToScroll\": 2,\r\n                \"responsive\": [\r\n                    {\r\n                        \"breakpoint\": 1025,\r\n                        \"settings\": {\r\n                            \"slidesToScroll\": 2,\r\n                            \"slidesToShow\": 2 \r\n                           \r\n                        }\r\n                    },\r\n                    {\r\n                        \"breakpoint\": 992,\r\n                        \"settings\": {\r\n                            \"slidesToScroll\": 2,\r\n                            \"slidesToShow\": 2\r\n                        }\r\n                    },\r\n                    {\r\n                        \"breakpoint\": 600,\r\n                        \"settings\": {\r\n                            \"slidesToScroll\": 1,\r\n                            \"slidesToShow\": 1\r\n                        }\r\n                    }\r\n                ]\r\n            }'>\r\n            {{#each blog.recent_posts}}\r\n            <div class=\"banner-item slick-slide\">\r\n                <div class=\"themevale_blog-wrapper\">\r\n                    <a class=\"banner-link\" href=\"{{this.url}}\">\r\n                        {{#if this.thumbnail}}\r\n                            <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage this.thumbnail 'homepage_blog_posts_img_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{this.title}}\">\r\n                        {{else}}\r\n                            <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/productdefault.gif\" alt=\"{{this.title}}\">\r\n                        {{/if}}\r\n                    </a>\r\n                    <div class=\"blog-content-bottom\">\r\n                            <div class=\"content-top-blog\">\r\n                                <a class=\"blog-title\" href=\"{{this.url}}\">{{ellipsis this.title 70}}</a>\r\n                                <div class=\"posted-by\"><span><svg class=\"icon\"><use xlink:href=\"#icon-user-blog\"></use></svg>{{#if this.author}}{{lang 'blog.posted_by' name=this.author}}</span>{{/if}}<span><svg class=\"icon\"><use xlink:href=\"#icon-clock-blog\"></use></svg>{{this.date_published}}</span></div>\r\n                            </div>\r\n                            <div class=\"content-bottom-blog\">\r\n                                <p>{{ellipsis this.summary 200}}...</p>\r\n                                <a href=\"{{this.url}}\" class=\"read-more-blog\"><svg><use xlink:href=\"#icon-down-top\"/></svg>Read More</a>\r\n                            </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            {{/each}}\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n"}
