{"components/blog/post":"<article class=\"blog\">\n    <div class=\"blog-post-body\">\n        <header class=\"blog-header\">\n            <h2 class=\"blog-title\">\n                <a href=\"{{post.url}}\">{{post.title}}</a>\n            </h2>\n            <p class=\"blog-date\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p>\n        </header>\n    </div>\n    <div class=\"blog-post-figure\">\n        {{#if post.thumbnail}}\n            <figure class=\"blog-thumbnail\">\n                <a href=\"{{post.url}}\">\n                    <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage post.thumbnail 'blog_size'}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\n                </a>\n            </figure>\n        {{/if}}\n    </div>\n    <div class=\"blog-post-body\">\n        <div class=\"blog-post\">\n            {{#if post.body}}\n                {{{post.body}}}\n            {{else}}\n                {{{post.summary}}}\n                {{#if post.show_read_more}}\n                    &hellip; \n                    <div class=\"readmore\">\n                        <a href=\"{{url}}\"><svg><use xlink:href=\"#icon-down-top\"/></svg>read more</a>\n                    </div>\n                {{/if}}\n\n            {{/if}}\n        </div>\n        {{#if show_tags}}\n            {{#if post.tags}}\n                <ul class=\"tags\">\n                    {{#each post.tags}}\n                    <li class=\"tag\">\n                        <a href=\"{{url}}\">#{{name}}</a>\n                    </li>\n                    {{/each}}\n                </ul>\n            {{/if}}\n        {{/if}}\n        {{> components/common/share}}\n    </div>\n</article>\n","components/common/share":"<div class=\"social-mediaBlock\">\n    <script type=\"text/javascript\" src=\"//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-564d3080dd24d74d\" async=\"async\"></script>\n    <!-- Go to www.addthis.com/dashboard to customize your tools -->\n    <div class=\"addthis_native_toolbox\"></div>\n</div>\n"}
